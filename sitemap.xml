---
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>{{ site.url }}/tag</loc>
        <changefreq>daily</changefreq>
        <priority>0.5</priority>
    </url>
    <url>
        <loc>{{ site.url }}/notes/root</loc>
        <changefreq>daily</changefreq>
        <priority>0.5</priority>
    </url>
    {% for note in site.notes %}
        {% unless note.title == 'root' %}
            <url>
                <loc>{{ site.url }}{{ note.url }}</loc>
                <lastmod>{{ note.updated | default: note.date | date_to_xmlschema }}</lastmod>
                <changefreq>{{ note.sitemap.changefreq | default: 'daily' }}</changefreq>
                <priority>{{ note.sitemap.priority | default: '0.5' }}</priority>
            </url>
        {% endunless %}
    {% endfor %}
</urlset>
