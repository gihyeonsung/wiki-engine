---
---
<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0">
  <channel>
    <title>{{ site.title | xml_escape }}</title>
    <link>{{ site.url }}</link>
    <description>{{ site.description | xml_escape }}</description>{% for note in site.notes limit:30 %}
    <item>
      <title>{{ note.title | xml_escape }}</title>
      <link>{{ site.url }}{{ note.url }}</link>
      <description>{{ note.content | xml_escape }}</description>
      <pubDate>{{ note.date | date_to_rfc822 }}</pubDate>
    </item>{% endfor %}
  </channel>
</rss>
